# Vercel Postgres Environment Variables
# Get these by running: vercel env pull .env.local

# Connection pooling (use this for Prisma queries)
POSTGRES_PRISMA_URL="postgresql://..."

# Direct connection (use this for migrations)
POSTGRES_URL_NON_POOLING="postgresql://..."

# Standard connection string
POSTGRES_URL="postgresql://..."

# Optional: For direct @vercel/postgres queries
POSTGRES_DATABASE="vercel-postgres"
POSTGRES_HOST="..."
POSTGRES_PASSWORD="..."
POSTGRES_USER="..."
